{
  "name": "react-twitch-streamers",
  "version": "0.0.2",
  "description": "React web app for displaying twitch streamers with the usage of Twitch API",
  "private": false,
  "homepage": "https://twitch-streams.netlify.com",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "--DEVELOPMENT--": "",
    "eject": "cross-env react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint": "tslint --config tslint.json --project tsconfig.json --force *.{ts,tsx}",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "posttest": "yarn test:t",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' 'cypress/**/*.{js,json}'",
    "start": "cross-env react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "cross-env CI=true react-scripts test --env=jsdom --no-cache",
    "test:cy": "cypress run",
    "test:cy:ci": "run-p --race start test:cy",
    "test:cy:w": "cypress open",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:w": "cross-env react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn lint && yarn format:d",
    "--BUILD--": "",
    "build": "cross-env react-scripts build",
    "build:ci": "yarn build",
    "prebuild:ci": "run-s validate test sweep",
    "--PRODUCTION--": "",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "axios": "0.19.0",
    "emotion": "10.0.17",
    "lodash": "4.17.15",
    "param-case": "2.1.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-router-dom": "5.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "4.1.0",
    "@testing-library/react": "9.1.3",
    "@types/jest": "24.0.18",
    "@types/lodash": "4.14.138",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-router-dom": "4.3.5",
    "cross-env": "5.2.1",
    "cypress": "3.4.1",
    "husky": "3.0.5",
    "jest-emotion": "10.0.17",
    "lint-staged": "9.2.5",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.1.1",
    "react-test-renderer": "16.9.0",
    "rimraf": "3.0.0",
    "serve": "11.1.0",
    "tslint": "5.19.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.6.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
