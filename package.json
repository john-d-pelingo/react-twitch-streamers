{
  "name": "react-twitch-streamers",
  "version": "0.0.2",
  "description": "React web app for displaying twitch streamers with the usage of Twitch API",
  "private": false,
  "homepage": "https://pensive-elion-127e92.netlify.com",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers/issues"
  },
  "scripts": {
    "--DEVELOPMENT--": "",
    "eject": "cross-env react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint": "tslint --config tslint.json --project tsconfig.json --force *.{ts,tsx}",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' 'cypress/**/*.{js,json}'",
    "start": "cross-env react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "cross-env CI=true react-scripts test --env=jsdom --no-cache",
    "test:cy": "cypress run",
    "test:cy:ci": "run-p --race start test:cy",
    "test:cy:w": "cypress open",
    "test:t": "tsc --noEmit",
    "test:w": "cross-env react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn lint && yarn format:d && yarn test:t",
    "--BUILD--": "",
    "build": "cross-env react-scripts build",
    "build:ci": "yarn build",
    "prebuild:ci": "run-s validate test test:t test:cy:ci sweep",
    "--PRODUCTION--": "",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "emotion": "^10.0.9",
    "lodash": "^4.17.11",
    "param-case": "^2.1.1",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.129",
    "@types/node": "^12.0.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router-dom": "^4.3.3",
    "cross-env": "^5.2.0",
    "cypress": "^3.3.0",
    "husky": "^2.3.0",
    "jest-dom": "^3.0.0",
    "jest-emotion": "^10.0.11",
    "lint-staged": "^8.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-scripts": "3.0.1",
    "react-test-renderer": "^16.7.0",
    "rimraf": "^2.6.3",
    "serve": "^11.0.0",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.2.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
