{
  "name": "react-twitch-streamers",
  "version": "0.0.2",
  "description": "React web app for displaying twitch streamers with the usage of Twitch API",
  "private": false,
  "homepage": "https://twitch-streams.netlify.com",
  "author": "John Darryl Pelingo <johndarrylpelingo@gmail.com> (https://github.com/john-d-pelingo)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers/issues"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "--DEVELOPMENT--": "",
    "eject": "react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "posttest": "yarn test:t",
    "posttest:cy": "yarn test:cy:t",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' 'cypress/**/*.{js,json,ts,tsx}'",
    "start": "react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "CI=true react-scripts test --env=jsdom --no-cache",
    "test:cy": "cypress run",
    "test:cy:ci": "run-p --race start test:cy",
    "test:cy:w": "cypress open",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:cy:t": "tsc -p ./cypress/tsconfig.json --noEmit",
    "test:w": "react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn format:d",
    "--BUILD--": "",
    "build": "react-scripts build",
    "build:ci": "react-scripts build",
    "prebuild": "run-s sweep validate test",
    "prebuild:ci": "run-s sweep validate test test:cy:ci",
    "--PRODUCTION--": "",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "emotion": "^10.0.9",
    "lodash": "^4.17.11",
    "param-case": "^3.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^5.0.0"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@testing-library/cypress": "^7.0.0",
    "@testing-library/jest-dom": "^4.0.0",
    "@testing-library/react": "^9.1.0",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.167",
    "@types/node": "^12.0.2",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__cypress": "^5.0.0",
    "cypress": "^6.0.0",
    "husky": "^3.0.2",
    "jest-emotion": "^10.0.11",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "react-scripts": "3.4.4",
    "react-test-renderer": "^17.0.0",
    "rimraf": "^3.0.0",
    "serve": "^11.3.2",
    "typescript": "^3.2.2",
    "webpack": "^5.31.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
