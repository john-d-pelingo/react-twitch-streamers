{
  "name": "react-twitch-streamers",
  "version": "0.0.2",
  "description": "React web app for displaying twitch streamers with the usage of Twitch API",
  "private": false,
  "homepage": "https://twitch-streams.netlify.com",
  "author": {
    "email": "johndarrylpelingo@gmail.com",
    "name": "John Darryl Pelingo",
    "url": "https://johndpelingo.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers.git"
  },
  "bugs": {
    "url": "https://github.com/john-d-pelingo/react-twitch-streamers/issues"
  },
  "engines": {
    "node": ">=14"
  },
  "prettier": "@jdp-dev/prettier-config",
  "scripts": {
    "--DEVELOPMENT--": "",
    "eject": "react-scripts eject",
    "format": "yarn pretty --write",
    "format:d": "yarn pretty --list-different",
    "lint:staged": "lint-staged",
    "nuke": "rimraf node_modules && yarn install",
    "posttest": "yarn test:t",
    "posttest:cy": "yarn test:cy:t",
    "pretty": "prettier --config .prettierrc 'src/**/*.{css,js,ts,tsx}' 'cypress/**/*.{js,json,ts,tsx}'",
    "start": "react-scripts start",
    "sweep": "rimraf build coverage",
    "test": "CI=true react-scripts test --env=jsdom --no-cache",
    "test:cy": "cypress run",
    "test:cy:ci": "run-p --race start test:cy",
    "test:cy:w": "cypress open",
    "test:t": "tsc -p ./tsconfig.json --noEmit",
    "test:cy:t": "tsc -p ./cypress/tsconfig.json --noEmit",
    "test:w": "react-scripts test --env=jsdom --no-cache --verbose=false",
    "validate": "yarn format:d",
    "--BUILD--": "",
    "build": "react-scripts build",
    "build:ci": "react-scripts build",
    "prebuild": "run-s sweep validate test",
    "prebuild:ci": "run-s sweep validate test test:cy:ci",
    "--PRODUCTION--": "",
    "start:prod": "serve -l 8080"
  },
  "keywords": [
    "create-react-app",
    "typescript"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "emotion": "^10.0.9",
    "lodash": "^4.17.21",
    "param-case": "^3.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@bahmutov/add-typescript-to-cypress": "^2.1.2",
    "@jdp-dev/commitlint-config": "^0.0.18",
    "@jdp-dev/eslint-config-typescript-react": "^0.0.18",
    "@jdp-dev/prettier-config": "^0.0.18",
    "@jdp-dev/tsconfig": "^0.0.18",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/lodash": "^4.14.170",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/testing-library__cypress": "^5.0.9",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "cypress": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "jest-emotion": "^10.0.32",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react-scripts": "3.4.4",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^12.0.0",
    "typescript": "^4.3.5",
    "webpack": "^5.42.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!<rootDir>/node_modules/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
